package handlingNumbers;

public class Counting_with_numbers {

	public static void main(String[] args) {
		int i = 34;
		int j = 56;
		int heltal = i * j + 56;
		System.out.println(heltal);
		double d = 45.78;
		double decimalTal = d / 2.23;
		System.out.println(decimalTal);
		System.out.println(heltal * decimalTal);
		int avhugget = (int) decimalTal; // hugg av decimalerna
		System.out.println(avhugget);
	}

}
