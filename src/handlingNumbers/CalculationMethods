package handlingNumbers;

public class CalculationMethods {

	public static int additionMethod (int value1, int value2){
				
		int additionResult = value1 + value2;
		return additionResult;
			
	}
	
	public static int subtractionMethod (int value1, int value2){
				
		int subtractionResult = value1 - value2;
		return subtractionResult;
				
	}
	
	public static int multiplicationMethod (int value1, int value2){
		
		int multiplicationResult = value1 * value2;
		return multiplicationResult;
				
	}
	
public static double divisionMethod (int value1, int value2){
		
		double divisionResult = (double)value1 / (double)value2;
		return divisionResult;
				
	}
	
	
	
}
